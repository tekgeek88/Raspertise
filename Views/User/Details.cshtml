@model Dictionary<string, object>

<div class="bg-primary m-1 p-1 text-white">
    <h4>User Details</h4>
</div>

<div class="container">
    <div class="row justify-content-md-center bg-dark m-auto">
        <table class="table table-sm table-dark table-striped table-responsive table-bordered m-1 p-1 w-auto">
            @foreach (var kvp in Model) {
                <tr>
                    <th>@kvp.Key</th><td>@kvp.Value</td>
                </tr>
            }
        </table>
    </div>
</div>
<div class="form-group p-1 m-1">
    <a class="btn btn-primary" asp-action="Edit">Edit</a>
    <a asp-controller="Home" asp-action="Index" class="btn btn-secondary">Cancel</a>
</div>
